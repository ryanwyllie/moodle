{{!
    @template core/form_test

    Moodle template for the input field in an autocomplate form element.

    Classes required for JS:
    * none

    Data attributes required for JS:
    * none

    Context variables required for this template:

    Example context (json):
    {}
}}
<link href="poly-input.html" rel="import">
<link href="poly-timezone.html" rel="import">
<link href="poly-editor.html" rel="import">

<form name="test-form">
    <input name="test1" value="dfsd" data-mdl-is="input"></input>
    <input name="test2" data-mdl-is="input_repeat"></input>
    <mdl-input name="test5"></mdl-input>
    <mdl-input-repeat name="test3"></mdl-input-repeat>
    <mdl-input-repeat name="test4"></mdl-input-repeat>

    {{> core/form_element_input {"name":"test5"} }}

    <poly-timezone name="polymer-timezone-thing"></poly-timezone>
    <poly-editor name="atto-editor"></poly-editor>
    <poly-editor name="atto-editor-2"></poly-editor>
</form>
{{#js}}
    require(['jquery', 'core/form'], function($, form) {
        var formElement = $('form[name="test-form"]');
        form.enhance(formElement);
    });
{{/js}}
