{{!
    @template core/form_test

    Moodle template for the input field in an autocomplate form element.

    Classes required for JS:
    * none

    Data attributes required for JS:
    * none

    Context variables required for this template:

    Example context (json):
    {}
}}
<link href="poly-editor.html" rel="import">

<form name="test-form">
    <input name="test1" value="dfsd" data-mdl-is="input"></input>
    <input name="test2" data-mdl-is="input_repeat"></input>
    <mdl-input name="test5"></mdl-input>
    <mdl-input-repeat name="test3"></mdl-input-repeat>
    <mdl-input-repeat name="test4"></mdl-input-repeat>
    <poly-editor name="server-rendered"></poly-editor>
</form>
<br/>
<br/>
<br/>
<br/>
<br/>
<div id="replace-me"></div>
{{#js}}
    require(['jquery', 'core/form', 'core/templates'], function($, form, templates) {
        var formElement = $('form[name="test-form"]');
        form.enhance(formElement);

        templates.render('core/form_test_2', {}).done(function (html, js) {
            templates.replaceNode($('#replace-me'), html, js);
        });
    });
{{/js}}
